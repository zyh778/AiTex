{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "AiTex",
  "version": "2.0.0",
  "identifier": "com.aitex.app",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:1420",
